---
import DetailPage from "../components/DetailPage.astro";
import LineUpImage from "../images/Elemente_Subpages_Lineup.png";
import ProgrammList from "../components/ProgrammList.svelte";
import bandsFriday from "../data/bandsFriday";
import bandsSaturday from "../data/bandsSaturday";
---

<!DOCTYPE html>
<html lang="de">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>kleinLaut 2023 - Lineup</title>
  </head>
  <body>
    <DetailPage image={LineUpImage} pageTitle="Lineup">
      <ProgrammList
        client:load
        bandsFriday={bandsFriday}
        bandsSaturday={bandsSaturday}
      />
    </DetailPage>
  </body>
</html>
